{
  "name": "react-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/michaelmcshinsky/react-starter",
  "dependencies": {
    "axios": "^0.21.1",
    "custom-env": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0"
  },
  "scripts": {
    "start:staging": "cross-env process.env.NODE_ENV='staging' npm-run-all server:prod",
    "start:prod": "cross-env process.env.NODE_ENV='production' npm-run-all server:prod",
    "start:qa": "cross-env process.env.NODE_ENV='qa' npm-run-all server:dev",
    "start": "cross-env process.env.NODE_ENV='development' npm-run-all server:dev",
    "server:prod": "webpack-dev-server --hot --mode=production",
    "server:dev": "webpack-dev-server --hot --mode=development",
    "build:staging": "cross-env NODE_ENV='staging' && rm -rf dist && webpack --mode=production",
    "build:prod": "cross-env NODE_ENV='prod' && rm -rf dist && webpack --mode=production",
    "build:qa": "cross-env NODE_ENV='qa' && rm -rf dist && webpack --mode=development",
    "build": "cross-env NODE_ENV='dev' && rm -rf dist && webpack --mode=development",
    "prod": "webpack --config webpack.prod.js -p",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:lint": "eslint src/**"
  },
  "author": "Michael McShinsky",
  "license": "ISC",
  "description": "React Starter",
  "devDependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@testing-library/react": "^10.4.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
